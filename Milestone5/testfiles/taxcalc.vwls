//Program 2 : Piecewise Tax Calculator

//take in float
flt income = scnFLT;
flt rate = 0.0;

//tax determiner
f(income < 10000){
    rate = 0.0;
}
f(income >= 10000){
    rate = 0.1;
    f(income >= 40000){
        rate = 0.2;
        f(income >= 80000){
            rate = 0.3;
        }
    }
}



flt tax = rate * income;
flt net = income - tax;

rate = rate * 100;
prnt("tax rate ", rate, "%");
prnt("tax amount ", tax);
prnt("net ", net);